        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Query to Retrive all the UPCs attached to a product" enabled="true">
          <stringProp name="TestPlan.comments">in this jdbc request we are making a call to mcom database to retrive all the UPCs attached to a product</stringProp>
          <stringProp name="dataSource">productid_with_upcs</stringProp>
          <stringProp name="queryType">Prepared Select Statement</stringProp>
          <stringProp name="query">select distinct upc.product_id from upc inner join product on upc.product_id=product.product_id where (upc.PID is not null and product.Master_flag=&apos;N&apos; and upc.archived_flag=&apos;N&apos; and upc.UPC_HALT_ID is null AND rownum &lt;=2) GROUP BY upc.product_id HAVING COUNT(upc.Product_id)&gt;1</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="variableNames"></stringProp>
          <stringProp name="resultVariable"></stringProp>
